{
  "name": "client",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "declarations": "npx tsc -p ./config/declarations.tsconfig.json",
    "ui:dev:start": "cross-env SERVE_PATH=/ PUBLIC_PATH=/ webpack-dev-server --color false --config config/webpack.config.js",
    "ui:dev:bundle": "cross-env SERVE_PATH=/ PUBLIC_PATH=/ webpack-cli --mode development --config config/webpack.config.js",
    "ui:int:bundle": "cross-env SERVE_PATH=/public/ PUBLIC_PATH=/app/ webpack-cli --env.BUILD_KIND=integration --mode development --config config/webpack.config.js",
    "ui:prod:bundle": "cross-env SERVE_PATH=/public/ PUBLIC_PATH=/app/ webpack-cli --mode production --env.BUILD_KIND=production --config config/webpack.config.js",
    "graphql": "graphql-codegen --config config/codegen.yml",
    "start": "run-s ui:dev:start",
    "build": "run-s codegen ui:prod:bundle",
    "daily": "run-s ui:dev:start",
    "codegen": "npx graphql-codegen -c ./config/codegen.yml"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@uifabric/experiments": "^7.23.26",
    "apollo-boost": "^0.4.8",
    "apollo-cache-inmemory": "^1.6.6",
    "graphql": "^15.0.0",
    "lodash": "^4.17.15",
    "office-ui-fabric-react": "^7.111.0",
    "react": "0.0.0-experimental-33c3af284",
    "react-apollo": "^3.1.5",
    "react-dom": "0.0.0-experimental-33c3af284",
    "react-router": "^6.0.0-alpha.4",
    "react-router-dom": "^6.0.0-alpha.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.13.5",
    "@graphql-codegen/typescript": "^1.13.5",
    "@graphql-codegen/typescript-document-nodes": "^1.13.5",
    "@graphql-codegen/typescript-operations": "^1.13.5",
    "@graphql-codegen/typescript-react-apollo": "^1.13.5",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.7",
    "awesome-typescript-loader": "^5.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "graphql-codegen": "^0.4.0",
    "html-webpack-plugin": "^4.3.0",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "scalajs-friendly-source-map-loader": "^0.1.5",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  }
}
