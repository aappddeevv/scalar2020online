{
  "name": "server",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "declarations": "npx tsc -p declarations.tsconfig.json",
    "postinstall": "./util/create-links.sh",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "build:ts": "tsc --build tsconfig.prod.json",
    "build:ts:watch": "cross-env TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling tsc -w",
    "start:prod:watch": "cross-env NODE_ENV=production nodemon -w dist dist/server.js",
    "start:prod": "cross-env NODE_ENV=production node dist/server.js",
    "serve:dev": "cross-env NODE_ENV=development nodemon -w dist dist/server.js",
    "start:dev": "run-s build:ts copy-all serve:dev",
    "start:dev:watch": "run-p start:dev build:ts:watch",
    "daily": "cross-env NODE_ENV=development run-s start:dev:watch",
    "daily:int": "cross-env NODE_ENV=integration run-s start:dev:watch",
    "start": "run-s start:dev:watch",
    "build": "cross-env NODE_ENV=production ./util/build.sh",
    "copy-public": "cp -r ./src/public ./dist",
    "copy-schema": "cp ./src/schema.graphql ./dist",
    "copy-all": "run-p copy-public copy-schema",
    "codegen": "npx graphql-codegen"
  },
  "keywords": [
    "graphql",
    "express",
    "apollo"
  ],
  "author": "aappddeevv",
  "license": "MIT",
  "dependencies": {
    "apollo-server-errors": "^2.4.1",
    "apollo-server-express": "^2.11.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.4",
    "config": "^3.3.1",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "debug": "^4.1.1",
    "depd": "^2.0.0",
    "express": "^4.17.1",
    "fast-glob": "^3.2.2",
    "graphql-type-json": "^0.3.1",
    "helmet": "^3.22.0",
    "lodash": "^4.17.15",
    "luxon": "^2.5.2",
    "memory-cache": "^0.2.0",
    "node-cache": "^4.2.0",
    "serve-favicon": "^2.5.0",
    "tslib": "^1.11.1",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.13.5",
    "@graphql-codegen/typescript": "^1.13.5",
    "@graphql-codegen/typescript-resolvers": "^1.13.5",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "0.0.36",
    "@types/config": "0.0.36",
    "@types/cors": "^2.8.4",
    "@types/cron": "^1.7.2",
    "@types/debug": "^4.1.2",
    "@types/express": "^4.17.1",
    "@types/graphql-type-json": "^0.3.2",
    "@types/helmet": "0.0.45",
    "@types/luxon": "^1.22.0",
    "@types/memory-cache": "^0.2.0",
    "@types/node": "^13.11.0",
    "@types/node-cache": "^4.1.3",
    "@types/request": "^2.48.1",
    "child_process": "^1.0.2",
    "cross-env": "^6.0.3",
    "fs-extra": "^8.1.0",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "shelljs": "^0.8.3",
    "tslint": "^5.20.1",
    "typedoc": "^0.14.2",
    "typescript": "^3.9.2"
  }
}
